{
    "name": "titoelpropio77/laravel-generator",
    "description": "GoodMeal laravel generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Modesto salda√±a",
            "email": "modestosaldam93@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "illuminate/support": " 8.* || 9.* || 10.*",
        "prettus/l5-repository": "^2.9.0",
        "laracasts/flash": "~3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    }    
}
